{
	"name": "coding-buddies-bot",
	"version": "2.0.0",
	"description": "A bot to help encourage Coding Buddies",
	"exports": "./index.js",
	"node": ">=16.0.0",
	"type": "module",
	"scripts": {
		"test": "ACTIVE_ENV=test && node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"dev": "NODE_ENV=development && nodemon --exec npm run run",
		"start": "npm run build && npm run run",
		"build": "npx tsc --noEmit",
		"run": "npx ts-node --esm ./src/index.ts",
		"deploy-commands": "npx ts-node --esm ./scripts/deploy-commands.ts",
		"seed-points": "npx ts-node ./scripts/seed-db.ts",
		"lint": "npx eslint"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/waream2/cb-streak-bot.git"
	},
	"author": "Earn",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/waream2/cb-streak-bot/issues"
	},
	"homepage": "https://github.com/waream2/cb-streak-bot#readme",
	"dependencies": {
		"@discordjs/builders": "^0.12.0",
		"@discordjs/rest": "^0.3.0",
		"countries-and-timezones": "^3.3.0",
		"discord.js": "^13.6.0",
		"dotenv": "^16.0.0",
		"html-entities": "^2.3.3",
		"js-beautify": "^1.14.7",
		"mongodb": "^4.6.0",
		"node-cron": "^3.0.2",
		"puppeteer": "^15.3.0"
	},
	"devDependencies": {
		"@types/chai": "^4.3.5",
		"@types/js-beautify": "^1.13.3",
		"@types/lodash": "^4.14.195",
		"@types/mocha": "^10.0.1",
		"@types/node": "^18.11.18",
		"@types/node-cron": "^3.0.7",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"chai": "^4.3.7",
		"eslint": "^8.42.0",
		"mocha": "^10.2.0",
		"nodemon": "^2.0.15",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5"
	},
	"nodemonConfig": {
		"ignore": [
			"db.json"
		]
	}
}
