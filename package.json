{
  "name": "streak-bot",
  "version": "1.0.0",
  "description": "A bot to help encourage Coding Buddies",
  "exports": "./index.js",
  "node": ">=16.0.0",
  "type": "module",
  "scripts": {
    "test": "ACTIVE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "start": "node ./src/index.js",
    "dev": "NODE_ENV=development nodemon ./src/index.js",
    "deploy-commands": "node deploy-commands.js",
    "prepare": "husky install",
    "seed-points": "node ./scripts/seedDb.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waream2/cb-streak-bot.git"
  },
  "author": "Earn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/waream2/cb-streak-bot/issues"
  },
  "homepage": "https://github.com/waream2/cb-streak-bot#readme",
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "html-entities": "^2.3.3",
    "js-beautify": "^1.14.7",
    "lodash": "^4.17.21",
    "mongodb": "^4.6.0",
    "node-cron": "^3.0.2",
    "puppeteer": "^15.3.0",
    "sinon": "^15.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "nodemonConfig": {
    "ignore": [
      "db.json"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint src",
      "prettier --config .prettierrc 'src/**/*.js' --write"
    ]
  }
}
